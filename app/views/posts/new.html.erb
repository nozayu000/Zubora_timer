<h1>新規投稿</h1>
<!-- form_with部分 -->
<%= form_with model:@post, local:true do |f| %>

  <h4>ニックネーム</h4>
  <%= f.text_field :user_id %>

  <h4>ワークアウトした時間</h4>
  <%= f.text_field :timer_id %>


  <% if user.public == 1 %>
    <%= f.submit '投稿',rankings_path %>
  <% else %>
    <%= f.submit '投稿', posts_path %>
  <% end %>
  <%= f.submit '投稿' %>

<% end %>